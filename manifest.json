{
    "manifest_version": 3,
    "name": "Browser snipper for Are.na",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/index.html"
    }
}