{
    "manifest_version": 3,
    "name": "Are.na Snip",
    "version": "1.0",
    "icons": {
        "128": "icon/logo.png"
      },
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "identity",
        "sidePanel"
    ],
    "host_permissions": [
        "https://dev.are.na/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "side_panel": {
        "default_path": "sidepanel/index.html"
      }
}